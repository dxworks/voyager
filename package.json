{
  "name": "@dxworks/voyager",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "dxworks",
    "dxw"
  ],
  "homepage": "https://github.com/dxworks/voyager#readme",
  "bugs": {
    "url": "https://github.com/dxworks/voyager/issues"
